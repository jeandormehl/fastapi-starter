{
  "dashboard": {
    "id": null,
    "uid": "default",
    "title": "Default Dashboard",
    "description": "Comprehensive monitoring dashboard for FastAPI applications with observability stack",
    "tags": ["fastapi", "observability", "python", "prometheus", "loki", "tempo"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "5s",
    "schemaVersion": 39,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "üöÄ Service Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"app\"}",
            "legendFormat": "Service Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "index": 0,
                    "text": "DOWN"
                  },
                  "1": {
                    "color": "green",
                    "index": 1,
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "none"
          }
        },
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 2,
        "title": "‚è±Ô∏è Application Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "application_uptime_seconds",
            "legendFormat": "Uptime",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "continuous-GrYlRd"},
            "custom": {
              "displayMode": "gradient-gauge",
              "orientation": "horizontal"
            }
          }
        },
        "gridPos": {"h": 4, "w": 4, "x": 4, "y": 0}
      },
      {
        "id": 3,
        "title": "üìä Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "continuous-BlPu"},
            "custom": {
              "displayMode": "gradient-gauge",
              "orientation": "horizontal"
            }
          }
        },
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 0}
      },
      {
        "id": 4,
        "title": "‚ö†Ô∏è Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"4..|5..\"}[5m])) / sum(rate(http_requests_total[5m])) * 100",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "orange", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 4, "x": 12, "y": 0}
      },
      {
        "id": 5,
        "title": "üî• P95 Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95 Latency",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "orange", "value": 0.5},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 4, "x": 16, "y": 0}
      },
      {
        "id": 6,
        "title": "üîÑ Active Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "process_open_fds",
            "legendFormat": "Open FDs",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "continuous-YlOrRd"}
          }
        },
        "gridPos": {"h": 4, "w": 4, "x": 20, "y": 0}
      },
      {
        "id": 7,
        "title": "üìà HTTP Request Duration Percentiles",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P90",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P99",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "opacity"
            },
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "P50"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            },
            {
              "matcher": {"id": "byName", "options": "P90"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}]
            },
            {
              "matcher": {"id": "byName", "options": "P95"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            },
            {
              "matcher": {"id": "byName", "options": "P99"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 8,
        "title": "üìä HTTP Requests by Status Code",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "stacking": {"mode": "normal"}
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "2.."},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            },
            {
              "matcher": {"id": "byRegexp", "options": "3.."},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}]
            },
            {
              "matcher": {"id": "byRegexp", "options": "4.."},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            },
            {
              "matcher": {"id": "byRegexp", "options": "5.."},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 9,
        "title": "üíª System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "system_cpu_percent",
            "legendFormat": "CPU %",
            "refId": "A"
          },
          {
            "expr": "system_memory_percent",
            "legendFormat": "Memory %",
            "refId": "B"
          },
          {
            "expr": "system_disk_percent",
            "legendFormat": "Disk %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 15,
              "gradientMode": "opacity"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "CPU %"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}]
            },
            {
              "matcher": {"id": "byName", "options": "Memory %"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            },
            {
              "matcher": {"id": "byName", "options": "Disk %"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "purple"}}]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 10,
        "title": "üß† Process Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Resident Memory",
            "refId": "A"
          },
          {
            "expr": "process_virtual_memory_bytes",
            "legendFormat": "Virtual Memory",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "opacity"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 11,
        "title": "üóëÔ∏è Python Garbage Collection",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(python_gc_collections_total[5m])",
            "legendFormat": "Gen {{generation}} Collections/sec",
            "refId": "A"
          },
          {
            "expr": "rate(python_gc_objects_collected_total[5m])",
            "legendFormat": "Gen {{generation}} Objects/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 12,
        "title": "üîó Database Connection Pool",
        "type": "timeseries",
        "targets": [
          {
            "expr": "prisma_pool_connections_open",
            "legendFormat": "Open Connections",
            "refId": "A"
          },
          {
            "expr": "prisma_pool_connections_idle",
            "legendFormat": "Idle Connections",
            "refId": "B"
          },
          {
            "expr": "prisma_pool_connections_busy",
            "legendFormat": "Busy Connections",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "stacking": {"mode": "normal"}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 13,
        "title": "üîç Database Query Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(prisma_client_queries_total[5m])",
            "legendFormat": "Client Queries/sec",
            "refId": "A"
          },
          {
            "expr": "rate(prisma_datasource_queries_total[5m])",
            "legendFormat": "Datasource Queries/sec",
            "refId": "B"
          },
          {
            "expr": "prisma_client_queries_active",
            "legendFormat": "Active Queries",
            "refId": "C"
          },
          {
            "expr": "prisma_client_queries_wait",
            "legendFormat": "Waiting Queries",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 14,
        "title": "‚è±Ô∏è Database Query Wait Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(prisma_client_queries_wait_histogram_ms_bucket[5m]))",
            "legendFormat": "P50 Wait Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(prisma_client_queries_wait_histogram_ms_bucket[5m]))",
            "legendFormat": "P95 Wait Time",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(prisma_client_queries_wait_histogram_ms_bucket[5m]))",
            "legendFormat": "P99 Wait Time",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      },
      {
        "id": 15,
        "title": "üìä Request Size Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_request_size_bytes_sum[5m]) / rate(http_request_size_bytes_count[5m])",
            "legendFormat": "Avg Request Size - {{handler}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36}
      },
      {
        "id": 16,
        "title": "üì§ Response Size Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_response_size_bytes_sum[5m]) / rate(http_response_size_bytes_count[5m])",
            "legendFormat": "Avg Response Size - {{handler}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36}
      },
      {
        "id": 17,
        "title": "üî• Top Endpoints by Request Count",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(rate(http_requests_total[5m])) by (handler, method))",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background",
              "inspect": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 44}
      },
      {
        "id": 18,
        "title": "‚ö° Slowest Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, avg(rate(http_request_duration_seconds_sum[5m])) by (handler, method) / avg(rate(http_request_duration_seconds_count[5m])) by (handler, method))",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background",
              "inspect": false
            },
            "unit": "s",
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44}
      }
    ],
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "5m",
            "value": "5m"
          },
          "hide": 0,
          "includeAll": false,
          "label": "Time Range",
          "multi": false,
          "name": "range",
          "options": [
            {"selected": false, "text": "1m", "value": "1m"},
            {"selected": true, "text": "5m", "value": "5m"},
            {"selected": false, "text": "15m", "value": "15m"},
            {"selected": false, "text": "30m", "value": "30m"},
            {"selected": false, "text": "1h", "value": "1h"}
          ],
          "query": "1m,5m,15m,30m,1h",
          "queryValue": "",
          "skipUrlSync": false,
          "type": "custom"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    }
  }
}
