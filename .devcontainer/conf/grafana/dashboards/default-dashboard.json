{
  "id": null,
  "title": "Observability Dashboard",
  "tags": ["fastapi", "observability", "prisma", "system"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "System Overview",
      "type": "row",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "CPU Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "system_cpu_percent",
          "legendFormat": "CPU %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1}
    },
    {
      "id": 3,
      "title": "Memory Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "system_memory_percent",
          "legendFormat": "Memory %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1}
    },
    {
      "id": 4,
      "title": "Disk Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "system_disk_percent",
          "legendFormat": "Disk %"
